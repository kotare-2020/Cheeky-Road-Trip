{
  "name": "boilerplate-react-webpack",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "webpack",
    "dev": "nodemon --watch server server/index & webpack --watch",
    "start": "node server/index",
    "test": "jest --noStackTrace --watch",
    "watch": "nodemon -e js,jsx -x npm test",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex"
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "nodemon": "^1.11.0",
    "sqlite3": "^4.0.4",
    "supertest": "^3.0.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "bcrypt": "^3.0.2",
    "cors": "^2.8.3",
    "dotenv": "^6.2.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "jest": "^24.1.0",
    "jsdom": "^13.2.0",
    "jsonwebtoken": "^8.4.0",
    "jsx-loader": "^0.13.2",
    "jwt-decode": "^2.2.0",
    "knex": "^0.16.3",
    "passport": "^0.4.0",
    "pg": "^7.8.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^16.8.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0",
    "superagent": "^4.1.0"
  }
}
